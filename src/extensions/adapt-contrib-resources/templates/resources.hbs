<div class="resources-inner" role="complementary">
	<div role="list">
	{{#each resources}}
	<div class="resources-item drawer-item {{_type}}">
		<a role="listitem" class="resources-item-open drawer-item-open" target="_blank" href="{{_link}}" tabindex="0" aria-label="{{lookup ../model._filterButtons _type}}. {{{title}}}. {{{description}}}">
			<div class="drawer-item-title">
				<div class="drawer-item-title-inner h5">{{{title}}}</div>
			</div>
			<div class="drawer-item-description">
				<div class="drawer-item-description-inner">{{{description}}}</div>
			</div>
		</a>
	</div>
	{{/each}}
	</div>
	<div class="aria-label a11y-ignore-focus prevent-default" tabindex="0" aria-label="{{_globals._extensions._resources.resourcesEnd}}"/>
</div>
